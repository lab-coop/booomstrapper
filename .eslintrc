{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
   "ecmaVersion": 6,
   "ecmaFeatures": {
        "modules": true,
        "module":  true
    },
   "sourceType": "module"
  },
  "rules": {
    "space-before-function-parent": 0,
    "max-len": ["warn", 120],
    "no-unused-expressions": 0
  }
}
